	<link rel="stylesheet" type="text/css" href="/stylesheets/MyFontsWebfontsKit.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/board.css">


	<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
	<!-- jQuery easing plugin -->
</head>
<body class='home'>
	<div class='container'>
		<div class='top_container'>
			<div class='user_line'>
			</div>
			<div class='logo_line'>
				<div class='logo_box'>
					<img src='/images/ceos_logo.png'/>
				</div>
			</div>
			<div class='menu_line'>
				<div class="menu_box">
					<div class="menu_name menu_about">
						ABOUT US
					</div>
					<div class="menu_name menu_board active">
						BOARD
					</div>
					<div class="menu_name menu_mem">
						MEMBERS
					</div>
					<div class="menu_name menu_act">
						ACTIVITIES
					</div>
					<div class="menu_name menu_rec">
						RECRUIT
					</div>
					<div class="menu_name menu_con">
						CONTACT
					</div>
				</div>
			</div>
		</div>
		<div class='bottom_container'>
			<div class='left_col'>
				<div class='left_menu left_menu_notice'>
					<div class='left_eng' style='width: 65px;'>
						Notice
					</div>
					<div class='left_kor'>
					공지 게시판
					</div>
				</div>
				<div class='left_menu left_menu_free'>
					<div class='left_eng' style='width: 45px;'>
						Free
					</div>
					<div class='left_kor'>
					자유 게시판
					</div>
				</div>
				<div class='left_menu left_menu_anony on_page'>
					<div class='left_eng' >
					Anonymous
					</div>
					<div class='left_kor'>
					익명 게시판
					</div>
				</div>
				<div class='left_menu left_menu_faq'>
					<div class='left_eng' style='width: 45px;'>
					FAQ
					</div>
					<div class='left_kor'>
					자주묻는 질문
					</div>
				</div>
				<div class='left_menu left_menu_qna'>
					<div class='left_eng' style='width: 45px;'>
					Q&A
					</div>
					<div class='left_kor'>
					질문 게시판
					</div>
				</div>
			</div>
			<div class='right_col'>
				<div class='right_top'>
					<div class='right_top_name' style='font-size:40px;'>
					ANONYMOUS
					</div>
					<div class='right_top_info'>
					CEOS의 익명 게시판 입니다.
					</div>
				</div>
				<div class='right_bot'>
					<div class='board_container inline'>
						<div class='board_box board_box_first inline'>
							<div class='board_name_line'>
								<div class='board_name board_name_1'>NO</div>
								<div class='board_name board_name_2'>분류</div>
								<div class='board_name board_name_3'>제목</div>
								<div class='board_name board_name_4'>작성자</div>
								<div class='board_name board_name_5'>조회수</div>
								<div class='board_name board_name_6'>작성일</div>
							</div>
						</div>
						<%for(var i = 0 ; i < board.length-1 ; i++){%>
							<div class='board_box board_text_line inline'>
							<div class='board_name_line'>
								<div class='board_name board_name_1'><%=board[i].anid%></div>
								<div class='board_name board_name_2'>익명</div>
								<div class='board_name board_name_3' onclick="anonyClick(<%=board[i].anid%>)"><%=board[i].title%> [<%=board[i].reply_num%>]</div>
								<div class='board_name board_name_4'>익명</div>
								<div class='board_name board_name_5'><%=board[i].view_num%></div>
								<div class='board_name board_name_6'><%=board[i].written_time%></div>
							</div>
							</div>
						<%}%>
						</div>
						<div class='board_box board_text_line board_box_last inline'>
							<%if(board.length!==0){%>
								<div class='board_name_line'>
								<div class='board_name board_name_1'><%=board[board.length-1].anid%></div>
								<div class='board_name board_name_2'>익명</div>
								<div class='board_name board_name_3' onclick="anonyClick(<%=board[board.length-1].anid%>)"><%=board[board.length-1].title%> [<%=board[i].reply_num%>]</div>
								<div class='board_name board_name_4'>익명</div>
								<div class='board_name board_name_5'><%=board[board.length-1].view_num%></div>
								<div class='board_name board_name_6'><%=board[board.length-1].written_time%></div>
							</div>
							<%}%>
						</div>
						<div class='board_data_info_box'>
							<div class='board_make_btn anony_make'>글쓰기</div>
							<div class='board_data_num_box'>
								<% var temp = totalNum%5;
								   var page;
									if(temp == 0){
										page = totalNum/5;
									} else {
										page = Math.floor(totalNum/5 + 1);
									}
									var pageGroup = Math.floor(page/5);
									var groupNum = Math.floor(pageNum/5);
									if(pageGroup == groupNum){
										for(var i = 1 ; i < page%5+1 ; i++){%>
											<div class='board_data_num' onclick="anonyPage(<%=groupNum*5+i%>)"><%=groupNum*5+i%></div>			
										<%}
									} else{
										for(var i = 1 ; i < 6 ; i++){%>
											<div class='board_data_num' onclick="anonyPage(<%=groupNum*5+i%>)"><%=groupNum*5+i%></div>	
										<%}
									}
								%>
							</div>
							<div class='board_data_search_box'>
								<div class='board_data_search_btn'>
									<img src='/images/glass.png'/>
								</div>
								<input type='text' placeholder='Search' name='board_search'/>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="/javascripts/index.js" type="text/javascript"></script>
	<script src="/javascripts/board.js" type="text/javascript"></script>
</body>
</html>
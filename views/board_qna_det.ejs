	<link rel="stylesheet" type="text/css" href="/stylesheets/MyFontsWebfontsKit.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/board.css">
	
	<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
	<!-- jQuery easing plugin -->
</head>
<body class='home'>
	<div class='container'>
		<div class='top_container'>
			<div class='user_line'>
			</div>
			<div class='logo_line'>
				<div class='logo_box'>
					<img src='/images/ceos_logo.png'/>
				</div>
			</div>
			<div class='menu_line'>
				<div class="menu_box">
					<div class="menu_name menu_about">
						ABOUT US
					</div>
					<div class="menu_name menu_board active">
						BOARD
					</div>
					<div class="menu_name menu_mem">
						MEMBERS
					</div>
					<div class="menu_name menu_act">
						ACTIVITIES
					</div>
					<div class="menu_name menu_rec">
						RECRUIT
					</div>
					<div class="menu_name menu_con">
						CONTACT
					</div>
				</div>
			</div>
		</div>
		<div class='bottom_container'>
			<div class='left_col'>
				<div class='left_menu left_menu_notice'>
					<div class='left_eng' style='width: 65px;'>
						Notice
					</div>
					<div class='left_kor'>
					공지 게시판
					</div>
				</div>
				<div class='left_menu left_menu_free'>
					<div class='left_eng' style='width: 45px;'>
						Free
					</div>
					<div class='left_kor'>
					자유 게시판
					</div>
				</div>
				<div class='left_menu left_menu_anony'>
					<div class='left_eng' >
					Anonymous
					</div>
					<div class='left_kor'>
					익명 게시판
					</div>
				</div>
				<div class='left_menu left_menu_faq'>
					<div class='left_eng' style='width: 45px;'>
					FAQ
					</div>
					<div class='left_kor'>
					자주묻는 질문
					</div>
				</div>
				<div class='left_menu left_menu_qna on_page'>
					<div class='left_eng' style='width: 45px;'>
					Q&A
					</div>
					<div class='left_kor'>
					질문 게시판
					</div>
				</div>
			</div>
			<div class='right_col'>
				<div class='right_top'>
					<div class='right_top_name'>
					Q&A
					</div>
					<div class='right_top_info'>
					CEOS의 질문 게시판 입니다.
					</div>
				</div>
				<div class='right_bot'>
					<div class='board_container inline'>
						<div class='board_box board_box_first inline'>
							<div class='board_name_line'>
								<div class='board_name board_name_1'><%=targetBoardInfo.qid%></div>
								<div class='board_name board_name_2'>QnA</div>
								<div class='board_name board_name_3'><%=targetBoardInfo.title%></div>
								<div class='board_name board_name_4'><%=targetBoardInfo.writerName%></div>
								<div class='board_name board_name_5'><%=targetBoardInfo.view_num + 1%></div>
								<div class='board_name board_name_6'><%=targetBoardInfo.written_time%></div>
							</div>
							<style>
							.board_text{
								display:block;
							}
							</style>
							
						</div>
						<div class='board_text' style='border-bottom:5px solid #d0d0d0;'>
							<div class='board_text_data'>
								<%=targetBoardInfo.content%>
							</div>
						</div>
						<div class='board_data_info_box'>
							<%for (var i = 0 ; i < targetBoardReply.length ; i++){%>
							<div class='board_data_detgul_box inline'>
								<div class='board_data_detgul inline'>
									<div class='detgul_name'><%=targetBoardReply[i].writerName%></div>
									<div class='detgul_text'><%=targetBoardReply[i].content%></div>
								</div>
							</div>
							<%}%>
							<div class='board_data_detgul_make_box'>
								<input class='qna_reply' type='text' />
								<div class='board_data_detgul_btn' onclick="qnaReply(<%=targetBoardInfo.qid%>)">등록하기</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<script src="/javascripts/index.js" type="text/javascript"></script>
	<script src="/javascripts/board.js" type="text/javascript"></script>
</body>
</html>